<!-- BEGIN TWITTER FEED -->
 
<hr>
<% begin %>
  <h2>Tweets by <a href="https://www.twitter.com/team461wbi">@Team461WBI</a></h2>
  <div class="well">
    <% Twitter.user_timeline("team461wbi", options = { :count => 10 } ).each do |tweet| %>
      <p class="tweet">
        <%= sanitize tweet.text %>
        <span class="tweet-info">
          @<%= tweet.from_user() %>
          <a href="https://twitter.com/<%= tweet.from_user() %>/statuses/<%= tweet.id() %>"><%= time_ago_in_words(tweet.created_at()) %>     ago </a>
        </span>
      </p>
    <% end %>
  </div>
<% rescue %>
<p> No internet connection </p>
<% end %>


<!-- END TWITTER FEED -->
